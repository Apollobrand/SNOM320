{% include 'headI.html' %}
<body>
{% include 'navbarI.html.twig' %}
<!-- Carousel
================================================== -->
<header id="myCarousel" class="carousel slide">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <div class="carousel-inner">
        {% for i in forCarousel %}
            <div class="item active">
                <div class="fill" style="background-image:url({{ i.carouselImage }});"></div>
                <div class="carousel-caption">
                    <h1>{{ i.carouselCategory }}</h1>
                    <p>{{ i.carouselTitle }}</p>
                    <p><a class="btn btn-lg btn-primary" href="#">LÃ¦s mere</a></p>
                </div>
            </div>
        {% endfor %}
    </div>
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</header>
<!-- ==================================================
END CAROUSEL -->
<!--- Cases --->
<section class="container-fluid allando-card" id="caseContainer">
    <h1> Cases </h1>
    <div class="row">
        {% for i in forCases %}
            <div class="col-md-4">
                <a href="{{ i.caseSource }}" class="btn">
                    <div class="card">
                        <div class="card-block">
                            <h3>{{ i.caseTitle }}</h3>
                        </div>
                        <div class="card-img-bottom">
                            <img src="{{ i.caseImage }}" alt="caseOne" title="caseOne"/>
                        </div>
                    </div>
                </a>
            </div>
        {% endfor %}
    </div>
</section>
{% include 'footer.html' %}
</body>
